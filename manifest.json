{
  "update_url": "https://clients2.google.com/service/update2/crx",

  "author": "BaketHunt",
  "background": {
    "scripts": ["js/util.js", "js/notification.js", "js/bg.js", "js/start.js"]
  },
  "content_security_policy": "script-src 'self' https://*.google.com https://*.gstatic.com https://*.google-analytics.com; object-src 'self'",
  "default_locale": "en",
  "description": "his extension allows you to lock your browser after a specified period of inactivity.",
  "icons": {
    "128": "logos/logo-94.png",
    "16": "logos/logo-94.png",
    "24": "logos/logo-94.png",
    "48": "logos/logo-94.png",
    "64": "logos/logo-94.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "49",
  "name": "BasketHunt Browser Locker",
  "options_page": "html/options.html",
  "commands": {
    "lock-the-browser": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Press to lock your browser"
    }
  },
  "permissions": [
    "tabs",
    "notifications",
    "contextMenus",
    "storage",
    "unlimitedStorage",
    "history",
    "*://google.com/*"
  ],
  "short_name": "Browser Locker",
  "version": "1.0.7"
}
